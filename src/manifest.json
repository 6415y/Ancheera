{
  "manifest_version": 2,

  "name": "gbflmao",
  "description": "o shit waddup",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "pages/popup/popup.html",
    "default_title": "ayyy"
  },
  
  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [{
    "matches": ["http://gbf.game.mbga.jp/*"],
    "js": ["jquery-3.0.0.min.js", "script.js"],
    "run_at": "document_end"
  }],

  "permissions": [
    "cookies",
    "http://gbf.game.mbga.jp/*",
    "notifications",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "devtools_page": "pages/devtools/devtools.html"
}